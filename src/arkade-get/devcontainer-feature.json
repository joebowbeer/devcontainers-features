{
    "id": "arkade-get",
    "version": "0.0.1",
    "name": "arkade-get",
    "description": "Installs arkade and then arkade installs the other specified CLIs.",
    "documentationURL": "https://github.com/joebowbeer/devcontainers-features/tree/main/src/arkade-get",
    "options": {
        "kustomize": {
            "type": "string",
            "default": "",
            "description": "kustomize version"
        }
    },
    "customizations": {
        "vscode": {
            "settings": {
                "github.copilot.chat.codeGeneration.instructions": [
                    {
                        "text": "This dev container includes arkade pre-installed and available on the `PATH`."
                    }
                ]
            }
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers/features/common-utils"
    ]
}
